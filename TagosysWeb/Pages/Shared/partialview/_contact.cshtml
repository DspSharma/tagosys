@*


<div class="bg-contact">
    <section class="container contact">
        <div data-aos="fade-up" class="container aos-init aos-animate">
            <div class="section-title">
                <h2>Contact</h2>
                <p>Have any query or just a big thank for us, visit our office and we will be more than happy to meet you. Alternatively, you may just fill the form here and we will get in touch with you right away! </p>
            </div>
            <div data-aos="fade-up" data-aos-delay="100" class="row aos-init aos-animate">
                <div class="col-lg-6 mt-2">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="info-box">
                                <a href="http://maps.google.com">
                                    <i aria-hidden="true"
                                       class="fa fa-map-marker"></i>
                                </a>
                                <h3>Our Address</h3>
                                <div class="row text-left ml-2 mr-2">
                                 
                                    <div class="col-md-6 ">
                                        <strong class="text-center">Registered Office</strong><br />
                                        <strong class="text-center">Jaipur, INDIA</strong> <br>
                                        <h6 id="registeredOfficeAddress"></h6>
                                    </div>
                                  
                                    <div class="col-md-6 "> 
                                        <strong class="text-center">Operational office</strong><br />
                                        <strong class="text-center">Jaipur, INDIA </strong> <br> 
                                        <h6 id="operationalOfficeAddress"></h6>
                                     
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="info-box tt mt-4">
                                <a href="mailto:tagosys@gmail.com">
                                    <i aria-hidden="true"
                                       class="fa fa-envelope-o"></i>
                                </a>
                                <h3>Email Us</h3>
                                <a href="mailto:tagosys@gmail.com" id="contactemail"></a>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="info-box mt-4">
                                <a href="tel:+918387922385">
                                    <i aria-hidden="true"
                                       class="fa fa-phone"></i>
                                </a>
                                <h3>Call Us</h3>
                                +91- <a href="tel:+918387922385" id="contactmobile"></a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 d-flex mt-2">
               

                    <form class="php-email-form w-100" method="post" action="~/Admin/Contact/AddUpdateContact" onsubmit="return addUpdateContact()">
                        <div class="form-row">
                            <div class="col form-group">
                                <input type="text" class="form-control" id="contactname" name="Name" placeholder="Your Name">
                              
                            </div>
                            <div class="col form-group">
                                <input type="email" class="form-control" id="email" name="Email" placeholder="Your Email">
                              
                            </div>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" id="contactsubject" name="subject" placeholder="Subject">
                          
                        </div>
                        <div class="form-group">
                            <textarea class="form-control" id="contactMessage" name="message" rows="5" placeholder="Message"></textarea>
                          
                        </div>
                       

                        <button type="submit">Send Message</button>
                    </form>

              
                </div>
            </div>
        </div>
    </section>
</div>

@await Component.InvokeAsync("Toaster")*@